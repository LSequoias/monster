{
  "name": "monster",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:client": "ng serve",
    "start:server": "nodemon -r dotenv/config server/server.js",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "compodoc:gen": "npx compodoc -p src/tsconfig.app.json",
